{
  "name": "book-search-backend",
  "version": "1.0.0",
  "description": "Backend for book finder app",
  "main": "server.js",
  "engines": {
    "node": "10.15.3"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"cd client && npm start\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Tyler Valley",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.5",
    "newrelic": "^5.11.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "radium": "^0.25.2",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
